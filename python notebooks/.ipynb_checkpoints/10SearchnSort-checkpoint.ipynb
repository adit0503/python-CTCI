   "source": [
    "def sortedMerge(A,B,n,m):\n",
    "    i,j,k = n-1,m-1,n+m-1\n",
    "    while(j > -1):\n",
    "        if i>=0 and A[i] >= B[j]:\n",
    "            A[k] = A[i]\n",
    "            i -= 1\n",
    "            k -= 1\n",
    "        else:\n",
    "            A[k] = B[j]\n",
    "            j -= 1\n",
    "            k -= 1\n",
    "    return A"
   ]
